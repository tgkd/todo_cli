$grey_blue = #696c9b
$grey_blue--hover = #7a7db2

.btn
  height 40px
  box-sizing border-box
  border none
  border-radius 2px

.btn--greyblue
  background-color $grey_blue
  &:hover
    background-color $grey_blue--hover

.btn-enter
  width 100%
  font-size 16px
  color #ffffff
  font-family "Roboto Thin"
  margin 30px 0 70px 0